{
    front
    {
        type            empty;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 247;
    }
    back
    {
        type            empty;
    }
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    cylinderback
    {
        type            fixedValue;
        value           nonuniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }
    region0_to_wallFilmRegion_wallFilmFaces
    {
        type            fixedValue;
        value           nonuniform 0;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 247;
    }
    procBoundary0to4
    {
        type            processor;
        value           uniform 247;
