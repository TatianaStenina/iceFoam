FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];


internalField   uniform 0.01;

boundaryField
{
    front
    {
        type            empty;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform 0;
    }
    back
    {
        type            empty;
    }
    top
    {
        type            slip;
    }
    bottom
    {
        type            slip;
    }
    cylinderback
    {
        type            kqRWallFunction;
        value           nonuniform 0;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0;
        value           nonuniform 0;
    }
    region0_to_wallFilmRegion_wallFilmFaces
    {
        type            kqRWallFunction;
        value           nonuniform 0;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.01;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 0.01;
    }
    procBoundary1to5
    {
        type            processor;
        value           uniform 0.01;
    }
}


