FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      htcConv;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -1 1 0 0 0];


internalField   uniform 0;

boundaryField
{
    front
    {
        type            empty;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform 0;
    }
    back
    {
        type            empty;
    }
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    cylinderback
    {
        type            fixedValue;
        value           nonuniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }
    region0_to_wallFilmRegion_wallFilmFaces
    {
        type            convectiveHeatTransfer;
        L               1;
        value           nonuniform 0;
    }
    procBoundary11to7
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary11to10
    {
        type            processor;
        value           uniform 0;
    }
}


